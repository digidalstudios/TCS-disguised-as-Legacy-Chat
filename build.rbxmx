                <string name="Source">script.Parent.Visible = false

local plr = game:GetService("Players").LocalPlayer
local char = plr.Character or plr.CharacterAdded:Wait()

local ChatSettings = require(game:GetService("TextChatService").LegacyChatService.ChatSettings)

if not ChatSettings.WindowResizable then script:Destroy() return end

local objectToScale = script.Parent.Parent
local chatbar = objectToScale.ChatBarParentFrame

script.Parent.Visible = true
chatbar.Size = UDim2.new(1, -44, 0, 42)

local uiDragDetector: UIDragDetector = Instance.new("UIDragDetector")
uiDragDetector.ResponseStyle = Enum.UIDragDetectorResponseStyle.Offset
uiDragDetector.CursorIcon = "rbxasset://textures/Cursors/KeyboardMouse/ArrowCursor.png"
uiDragDetector.ActivatedCursorIcon = "rbxasset://textures/Cursors/KeyboardMouse/ArrowCursor.png"
uiDragDetector.Parent = script.Parent

local minScale = Vector2.new(ChatSettings.MinimumWindowSize.X.Scale, ChatSettings.MinimumWindowSize.Y.Scale)
local maxScale = Vector2.new(ChatSettings.MaximumWindowSize.X.Scale, ChatSettings.MaximumWindowSize.Y.Scale)

local dragStartScaleX
local dragStartWidthX = 1
local dragStartScaleY
local dragStartWidthY = 1

uiDragDetector.DragStart:Connect(function()
	dragStartScaleY = objectToScale.Size.Y.Scale
	dragStartWidthY = objectToScale.AbsoluteSize.Y
	dragStartScaleX = objectToScale.Size.X.Scale
	dragStartWidthX = objectToScale.AbsoluteSize.X
end)

uiDragDetector.DragContinue:Connect(function()
	local heightDragged = uiDragDetector.DragUDim2.Y.Offset / dragStartWidthY
	local scaleToApplyY = dragStartScaleY * (1 + heightDragged)

	scaleToApplyY = math.clamp(scaleToApplyY, minScale.Y, maxScale.Y)

	objectToScale.Size = UDim2.new(objectToScale.Size.X.Scale, 0, scaleToApplyY, 0)
	
	local widthDragged = uiDragDetector.DragUDim2.X.Offset / dragStartWidthX
	local scaleToApplyX = dragStartScaleX * (1 + widthDragged)

	scaleToApplyX = math.clamp(scaleToApplyX, minScale.X, maxScale.X)

	objectToScale.Size = UDim2.new(scaleToApplyX, 0, objectToScale.Size.Y.Scale, 0)
	
	script.Parent.Position = UDim2.new(1, -42, 1, -42)
end)</string>
